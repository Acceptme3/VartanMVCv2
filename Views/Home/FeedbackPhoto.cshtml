@using VartanMVCv2.ViewModels
@using VartanMVCv2.Models
@using VartanMVCv2.Domain.Entities
@model IndexViewModel

@await Html.PartialAsync("HeaderWithoutSliderPartial")

<div class="parent_box">
    <h6 class="furnitures_text">@Model.CompletedProjectExample!.Title</h6>
    <p class="furnitures_text_photo">@Model.CompletedProjectExample.Description </p>
    <div class="grid-container_photo_box" >
        @foreach (CompletedProjectPhoto photo in Model.CompletedProjectExample!.Photos)
        {
            if (photo.Project!.ID == ViewBag.SelectedCompletedProjectID)
            {
                <img src="@photo.ImagePath" data-fancybox="gallery" data-caption = "@photo.Description"/>
            }          
        }
    </div>
    <div class="contact_bt_main">
        <div class="contact_bt"><a href="@Url.Action("Feedback", "Home")">Назад</a></div>
    </div>
</div>

@await Html.PartialAsync("FooterPartial")
<!-- copyright section start -->
<div class="copyright_section" />
<div class="container">
    @await Html.PartialAsync("AutorPartial")
</div>
<!-- copyright section end -->
@await Html.PartialAsync("ScriptsPartial")